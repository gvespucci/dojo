<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Telephone Directory</display-name>
 <servlet>
  <servlet-name>TelephoneDirectory</servlet-name>
  <display-name>TelephoneDirectory</display-name>
  <servlet-class>org.fao.teldir.servlet.TelephoneDirectoryServlet</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>TelephoneDirectory</servlet-name>
  <url-pattern>/teldir</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
 </welcome-file-list>
</web-app>
